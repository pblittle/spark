{
  "name": "@buildonspark/spark-frost-bare-addon",
  "version": "0.0.3",
  "description": "Example Bare addon",
  "exports": {
    "./package": "./package.json",
    ".": "./index.js"
  },
  "files": [
    "index.js",
    "binding.c",
    "binding.js",
    "CMakeLists.txt",
    "prebuilds"
