  "installConfig": {
    "hoistingLimits": "dependencies"
  },
  "devDependencies": {
    "@noble/curves": "^1.8.0",
    "bare": "^1.21.3",
    "bare-crypto": "^1.9.2",
    "bare-fs": "^4.1.6",
    "bare-make": "^1.5.1",
    "cmake-bare": "^1.6.5",
    "cmake-cargo": "^0.0.5",
    "cmake-npm": "^1.1.0",
    "prettier": "^3.5.1",
    "prettier-config-standard": "^7.0.0"
